#!/home/rafael/trybe-exercicios/ciencia-da-computacao/1-introducao-a-python/1.3-testes/fixacao/.venv/bin/python3
# -*- coding: utf-8 -*-
import re
import sys
from pytest import console_main
if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw|\.exe)?$', '', sys.argv[0])
    sys.exit(console_main())
